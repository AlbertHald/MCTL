to MoveAndPlace (amount: NUMBER, color: STRING): NOTHING {
  repeat(amount){
    placeUnder(color);
    moveForward();
  }
}

to MoveBack (amount: NUMBER): NOTHING {
  repeat(amount){
    moveBackward();
  }
}

